<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainScreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#52FFFFFF"
    tools:context=".MainScreen">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnSos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="20"
        android:background="#52FFFFFF"
        android:backgroundTint="#9A9A9A"
        android:backgroundTintMode="src_in"
        android:clickable="true"
        android:contentDescription="@android:string/emptyPhoneNumber"
        android:focusable="true"
        android:src="@android:drawable/ic_menu_call"
        android:tint="#FFFFFF"
        android:visibility="visible"
        app:backgroundTint="#C4FFFFFF"
        app:backgroundTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.928" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnSettings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="20"
        android:background="#52FFFFFF"
        android:backgroundTint="#9A9A9A"
        android:backgroundTintMode="src_in"
        android:clickable="true"
        android:contentDescription="@android:string/emptyPhoneNumber"
        android:focusable="true"
        android:src="@drawable/settings"
        android:tint="#FFFFFF"
        android:visibility="visible"
        app:backgroundTint="#C4FFFFFF"
        app:backgroundTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnZvuk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="25"
        android:background="#52FFFFFF"
        android:backgroundTint="#9A9A9A"
        android:backgroundTintMode="src_in"
        android:clickable="true"
        android:contentDescription="@string/soundOn"
        android:focusable="true"
        android:onClick="hideButton"
        android:src="@android:drawable/ic_lock_silent_mode_off"
        android:tint="#FFFFFF"
        android:visibility="visible"
        app:backgroundTint="#C4FFFFFF"
        app:backgroundTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.928" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnZvukOff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="25"
        android:background="#52FFFFFF"
        android:backgroundTint="#9A9A9A"
        android:backgroundTintMode="src_in"
        android:clickable="true"
        android:contentDescription="@string/soundOff"
        android:focusable="true"
        android:onClick="showButton"
        android:src="@android:drawable/ic_lock_silent_mode"
        android:tint="#FFFFFF"
        android:visibility="gone"
        app:backgroundTint="#C4FFFFFF"
        app:backgroundTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.928" />


        <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

            <ImageView
                android:id="@+id/mainCar"
                android:layout_width="213dp"
                android:layout_height="278dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="116dp"
                android:layout_marginBottom="283dp"
                android:background="@null"
                android:contentDescription="@string/mainScreenCar"
                app:srcCompat="@drawable/auto" />

            <ImageView
            android:id="@+id/idSenzor1Lvl3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            app:srcCompat="@drawable/senzor1lvl3" />

        <ImageView
            android:id="@+id/idSenzor1Lvl2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"


            app:srcCompat="@drawable/senzor1lvl2" />

        <ImageView
            android:id="@+id/idSenzor1Lvl1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            app:srcCompat="@drawable/senzor1lvl1" />


        <ImageView
            android:id="@+id/idSenzor2Lvl3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            app:srcCompat="@drawable/senzor2lvl3" />


            <ImageView
                android:id="@+id/idSenzor2Lvl2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"

                app:srcCompat="@drawable/senzor2lvl2" />

            <ImageView
                android:id="@+id/idSenzor2Lvl1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="244dp"
                android:layout_marginBottom="362dp"
                app:srcCompat="@drawable/senzor2lvl1" />

            <ImageView
            android:id="@+id/idSenzor3Lvl3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/senzor3lvl3" />
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"

            <ImageView
                android:id="@+id/idSenzor3Lvl2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"

                android:layout_marginRight="146dp"
                android:layout_marginBottom="296dp"
                app:srcCompat="@drawable/senzor3lvl2" />

            <ImageView
                android:id="@+id/idSenzor3Lvl1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="266dp"
                android:layout_marginBottom="273dp"
                app:srcCompat="@drawable/senzor3lvl1" />

            <ImageView
                android:id="@+id/idSenzor4Lvl3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="185dp"
                android:layout_marginBottom="171dp"
                app:srcCompat="@drawable/senzor4lvl3" />

            <ImageView
                android:id="@+id/idSenzor4Lvl2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="76dp"
                android:layout_marginBottom="210dp"
                app:srcCompat="@drawable/senzor4lvl2" />

            <ImageView
                android:id="@+id/idSenzor4Lvl1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="169dp"
                android:layout_marginBottom="220dp"
                app:srcCompat="@drawable/senzor4lvl1" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="298dp"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="56dp"
                android:layout_marginBottom="230dp"
                android:text="TextView" />


        </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>